{
  "name": "The Builder",
  "short_name": "Builder",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#ff4b4b",
  "icons": [
    {
      "src": "icon-192.png",
      "sizes": "192x192",
      "type": "image/png"
    }
  ],
  "share_target": {
    "action": "/share/exercise_image",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "exercise": "exercise",
      "files": [
        {
          "name": "file",
          "accept": ["image/*"]
        }
      ]
    }
  }
}
